{
  "scripts": {
    "start": "npm run build_dev && live-server --port=7777",
    "test": "karma start",
    "testcafe": "concurrently \"http-server\" \"testcafe chrome testCafe/\"",
    "testcafe_sauce": "testcafe \"saucelabs:Chrome@56.0:Windows 10\" testCafe/",
    "release": "standard-version --message \"Release: %s\" ",
    "build_knockout_dev": "webpack --env.buildType dev --env.platform knockout",
    "build_react_dev": "webpack --env.buildType dev --env.platform react",
    "build_angular_dev": "webpack --env.buildType dev --env.platform angular",
    "build_jquery_dev": "webpack --env.buildType dev --env.platform jquery",
    "build_vue_dev": "webpack --env.buildType dev --env.platform vue",
    "build_dev": "concurrently \"npm run build_knockout_dev\" \"npm run build_react_dev\" \"npm run build_angular_dev\" \"npm run build_jquery_dev\" \"npm run build_vue_dev\"",
    "build_knockout_prod": "webpack --env.buildType prod --env.platform knockout",
    "build_react_prod": "webpack --env.buildType prod --env.platform react",
    "build_angular_prod": "webpack --env.buildType prod --env.platform angular",
    "build_jquery_prod": "webpack --env.buildType prod --env.platform jquery",
    "build_vue_prod": "webpack --env.buildType prod --env.platform vue",
    "build_prod": "npm run build_dev && concurrently \"npm run build_knockout_prod\" \"npm run build_react_prod\" \"npm run build_angular_prod\" \"npm run build_jquery_prod\" \"npm run build_vue_prod\"",
    "doc_gen": "set TS_NODE_COMPILER_OPTIONS={\"module\":\"commonjs\"} && ts-node doc_generator/lib_docgenerator.ts src/entries/chunks/model.ts",
    "doc_update": "set TS_NODE_COMPILER_OPTIONS={\"module\":\"commonjs\"} && ts-node doc_generator/lib_docupdater.ts",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "watch_knockout_dev": "webpack --env.buildType dev --env.platform knockout --watch",
    "watch_react_dev": "webpack --env.buildType dev --env.platform react --watch",
    "watch_angular_dev": "webpack --env.buildType dev --env.platform angular --watch",
    "watch_jquery_dev": "webpack --env.buildType dev --env.platform jquery --watch",
    "watch_vue_dev": "webpack --env.buildType dev --env.platform vue --watch",
    "watch_svelte_dev": "webpack --env.buildType dev --env.platform svelte --watch"
  },
  "version": "1.0.53",
  "name": "surveyjs",
  "private": true,
  "devDependencies": {
    "@types/angular": "1.6.2",
    "@types/jquery": "2.0.39",
    "@types/knockout": "3.4.46",
    "@types/node": "7.0.4",
    "@types/qunit": "2.0.31",
    "@types/react": "16.4.2",
    "@types/react-dom": "^16.0.4",
    "ajv": "4.11.2",
    "concurrently": "3.1.0",
    "css-loader": "^1.0.0",
    "dotenv": "4.0.0",
    "dts-bundle": "0.7.2",
    "generate-json-webpack-plugin": "0.2.1",
    "github-api": "^3.0.0",
    "html-loader": "0.4.4",
    "http-server": "0.9.0",
    "jquery": "3.1.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-junit-reporter": "1.0.0",
    "karma-qunit": "1.2.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.2",
    "knockout": "3.4.0",
    "live-server": "1.2.0",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "4.9.2",
    "node-uuid": "1.4.7",
    "prettier": "~1.12.0",
    "puppeteer": "^1.0.0",
    "qunitjs": "2.2.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "replace-in-file": "^3.1.1",
    "rimraf": "2.5.4",
    "sass-loader": "^7.1.0",
    "standard-version": "4.0.0",
    "style-loader": "0.13.1",
    "surveyjs-doc-generator": "git+https://github.com/surveyjs/surveyjs-doc-generator.git",
    "svelte": "^2.13.5",
    "svelte-loader": "^2.11.0",
    "testcafe": "0.15.0",
    "testcafe-browser-provider-saucelabs": "1.1.0",
    "ts-loader": "^5.2.1",
    "ts-node": "3.3.0",
    "tslint": "4.5.1",
    "typescript": "2.6.1",
    "url-loader": "^0.6.2",
    "val-loader": "0.5.0",
    "vue": "^2.5.17",
    "vue-class-component": "^6.2.0",
    "vue-loader": "^15.4.2",
    "vue-property-decorator": "^7.2.0",
